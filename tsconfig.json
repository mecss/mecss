{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "./build",
        "baseUrl": ".",
        "moduleResolution": "node",
    },
    "include": [
        "./lib/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
